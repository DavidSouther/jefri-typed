{
    "compilerOptions": {
        "declaration": true,
        "experimentalDecorators": true,
        "module": "commonjs",
        "noEmitOnError": true,
        "noImplicitAny": true,
        "outDir": "./dist",
        "preserveConstEnums": true,
        "rootDir": "./src",
        "sourceMap": true,
        "suppressImplicitAnyIndexErrors": true,
        "target": "es5"
    }
}
